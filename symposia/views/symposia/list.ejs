<%- include('../partials/header') %>

<div class="container">
    <h1 class="text-align center">list of all symposia</h1>
    <table class="centered highlight responsive-table">
        <tbody>
          <% symposium.forEach( (s) => { %>
            <tr>
              <td><a href="/symposia/<%=s._id%>"><%= s.name %></a></td>
            </tr>
            <% }); %>
        </tbody>
    </table>      
</div>

<%- include('../partials/footer') %>